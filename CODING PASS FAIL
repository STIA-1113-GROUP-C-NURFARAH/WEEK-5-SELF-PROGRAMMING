package selfProgramming_1;

import java.util.Scanner;

public class PassFail {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Double selection using if/else statement
		
		Scanner sc = new Scanner(System.in);
			
			System.out.println("Enter your mark: "); //print to enter mark
			double mark = sc.nextDouble();
			
			if(mark>=40) //read mark
				System.out.println("PASS"); //print message
			else
				System.out.println("FAIL"); //print message
	}

}
